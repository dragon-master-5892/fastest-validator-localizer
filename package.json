{
  "name": "fastest-validator-localizer",
  "version": "1.0.0",
  "description": "message localizer for icebob/fastest-validator",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "pretty": "prettier --write \"src/**/*.ts\" \"lib/**/*.js\" \"tests/**/*.ts\"",
    "lint": "gts lint",
    "prepare": "npm run compile",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run pretty && git add -A src",
    "postversion": "git push && git push --tags",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dragon-master-5892/fastest-validator-localizer.git"
  },
  "keywords": [
    "fastest-validator",
    "fastest-validator-locale",
    "fastest-validator-messages"
  ],
  "author": "Amirhossein Saffari (https://github.com/dragon-master-5892)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dragon-master-5892/fastest-validator-localizer/issues"
  },
  "homepage": "https://github.com/dragon-master-5892/fastest-validator-localizer#readme",
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "gts": "^3.1.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.7.0",
    "@types/node": "^14.11.2"
  },
  "readmeFilename": "README.md",
  "webpack": "./dist/index.js",
  "browser": "./dist/index.js",
  "files": [
    "lib/**/*",
    "LICENSE"
  ]
}
